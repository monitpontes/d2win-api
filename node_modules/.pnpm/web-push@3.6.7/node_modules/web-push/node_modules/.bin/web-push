#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules/web-push/src/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules/web-push/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules/web-push/src/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules/web-push/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/web-push@3.6.7/node_modules:/mnt/c/Users/jmroberto/Documents/VSCode/Monitoramento/d2win_api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli.js" "$@"
else
  exec node  "$basedir/../../src/cli.js" "$@"
fi
